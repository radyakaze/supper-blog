{
  "name": "supper-blog",
  "description": "Simple static generator powered by Sapper",
  "version": "0.0.1",
  "scripts": {
    "dev": "sapper dev",
    "build": "sapper build --legacy",
    "export": "sapper export --legacy --entry '/ /crawl'",
    "start": "node __sapper__/build",
    "validate": "svelte-check --ignore src/node_modules/@sapper",
    "sass:watch": "node-sass --watch src/styles -o static/styles --output-style compressed",
    "sass:build": "node-sass src/styles -o static/styles --output-style compressed"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "dayjs": "^1.9.4",
    "html-substring": "^1.8.1",
    "polka": "next",
    "sirv": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "@rollup/plugin-url": "^5.0.0",
    "@tsconfig/svelte": "^1.0.10",
    "@types/compression": "^1.7.0",
    "@types/node": "^14.11.1",
    "@types/polka": "^0.5.1",
    "@types/slug": "^0.9.1",
    "autoprefixer": "^10.0.1",
    "bulma": "^0.9.1",
    "gray-matter": "^4.0.2",
    "marked": "^1.2.3",
    "node-sass": "^5.0.0",
    "postcss": "^8.1.6",
    "prismjs": "^1.22.0",
    "read-time-estimate": "^0.0.2",
    "rollup": "^2.3.4",
    "rollup-plugin-glob": "^1.0.2",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sanitize-html": "^2.1.2",
    "sapper": "^0.28.0",
    "slug": "^4.0.2",
    "svelte": "^3.17.3",
    "svelte-check": "^1.0.46",
    "svelte-intersection-observer": "^0.1.1",
    "svelte-preprocess": "^4.5.2",
    "tslib": "^2.0.1",
    "typescript": "^4.0.3"
  },
  "author": "Radya",
  "license": "MIT"
}
